#!/usr/bin/env lua
--/bin/nano: an alternative to program a good text editor.

_handler = {}
_handler.signal_handler = function (sig)
    if sig == 'kill' then
        os.debug.debug_write("nano: SIGKILL'd!", false)
        return 0
    end
end

function main(args)
    local file = args[1]
    local h = fs.open(os.cshell.resolve(file), 'r')
    h.close()
end

main({...})
