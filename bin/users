#!/usr/bin/env lua
--/bin/users: says what users are logged

-- TODO: logged users list

_handler = {}
_handler.signal_handler = function (sig)
    if sig == 'kill' then
        --print("users: recieved SIGKILL")
        return 0
    end
end

function main(args)
    local cuser = fs.open("/tmp/current_user", 'r')
    local current_user = cuser.readAll()
    cuser.close()

    print(current_user)
end
